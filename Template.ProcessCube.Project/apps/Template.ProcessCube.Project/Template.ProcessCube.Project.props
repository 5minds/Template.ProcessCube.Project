<Project>
    <PropertyGroup>
        <!-- SpaYarp configuration -->
        <SpaRoot>frontend\</SpaRoot>
        <!-- TARGET Properties-->
        <PackageLockJsonFile>frontend\package-lock.json</PackageLockJsonFile>
        <NodeModulesPath>frontend\node_modules</NodeModulesPath>
        <AtlasEngineDbFile>../../.processcube/engine/db/atlasengine.sqlite</AtlasEngineDbFile>
        <AppName>$(MSBuildProjectName)</AppName>
        <DockerRoot>../../</DockerRoot>
        <DockerImageName>$(MSBuildProjectName.ToLower().Replace(".","_"))</DockerImageName>
        <SpaClientUrl>http://frontend:3000</SpaClientUrl>
        <TestResultsPath>../../testresults</TestResultsPath>
    </PropertyGroup>
</Project>
